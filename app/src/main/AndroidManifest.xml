<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mistrio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BatataDica">
            <intent-filter>
                <action android:name="android.intent.action.DICA"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".InstrucoesActivity">
            <intent-filter>
                <action android:name="android.intent.action.INSTRUCOES"/>
            </intent-filter>
        </activity>

        <activity android:name=".CozinhandoActivity">
            <intent-filter>
                <action android:name="android.intent.action.FASE1"/>
            </intent-filter>
        </activity>

        <activity android:name=".CozinhandoFinal">
            <intent-filter>
                <action android:name="android.intent.action.FASE1_FINAL"/>
            </intent-filter>
        </activity>

        <activity android:name=".Pais">
            <intent-filter>
                <action android:name="android.intent.action.FASE2"/>
            </intent-filter>
        </activity>

        <activity android:name=".PaisDica">
            <intent-filter>
                <action android:name="android.intent.action.FASE2_DICA"/>
            </intent-filter>
        </activity>

        <activity android:name=".PaisFinal">
            <intent-filter>
                <action android:name="android.intent.action.FASE2_FINAL"/>
            </intent-filter>
        </activity>

        <activity android:name=".Tela_inicialFinal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN_FINAL"/>
            </intent-filter>
        </activity>

        <activity android:name="Final_Jogo">
            <intent-filter>
                <action android:name="android.intent.action.FINAL"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
